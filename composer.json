{
    "name": "xactsystems/js-routing",
    "description": "A JS routing bundle for Symfony 3.4+ using webpack. Derived from the FOS JSRoutingBundle.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Ian Foulds",
            "email": "ianfoulds@x-act.co.uk"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.1-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Xact\\JSRoutingBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xact\\JSRoutingBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0",
        "symfony/asset": "~4.0 || ~5.0",
        "symfony/flex": "~1.3",
        "symfony/form": "~4.1.9 || ~5.0",
        "symfony/framework-bundle": "~4.1.12 || ~5.0",
        "symfony/serializer-pack": "~1.0",
        "symfony/twig-pack": "~1.0",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "~4.0 || ~5.0"
    },
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*"
    }
}
